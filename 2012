#include<iostream>
using namespace std;
class Algebra
{
private:
  int a;
  int b;
  int count=0;
public:
    //Default Constructor
	  Algebra ()
  {
    a = 0;
    b = 0;
  }
  //Parametrize Constructor
    Algebra (int a, int b)
  {
       if(a>0&&b>0)
       {
    this->a = a;
    this->b = b;
    
       }
       else
       cout << "The Value of A & B is not positive or 0";
  }
// Copy Constructor
    Algebra (const Algebra & c)
  {
    a = c.a;
    b = c.b;

  }

  // Setter
  void  setA (int x)
  {
    if (x >= 0)
      a = x;
    else
      cout << "The Value of A is not positive or 0";

  }
 void setB (int y)
  {
    if (y >= 0)
      a = y;
    else
      cout << "The Value of B is not positive or 0";

  }

  //Getter
void  putData ()
  {
    cout << "Yhe Value of A is \t" << a;
    cout << endl << "The Value of B is\t" << b;

  }

  //- Operator Overloading  

 Algebra operator-(Algebra v)
  {
      Algebra temp;
      temp.a=a-v.a;
      temp.b=b-v.b;
      return temp;
  }
  

// + Uniriy Operator overload
bool operator+()
{
    if(a>0&&b>0)
    {
        return true;
    }
    else 
    return false;
}
// count method
  int  CountEqual(Algebra a[5],Algebra &temp)
    {
        for(int i=0;i<5;i++)
        {
            if(a[i]==temp)
            count++;
        }
        return count;
    }
    
//"==" Operator Overloading

bool operator==(Algebra obj)
{
	if(obj.a==a&&obj.b)
	return true;
	else
	return false;
}
friend istream & operator>>(istream &input,Algebra &p);
};
istream & operator>>(istream &input,Algebra &p)
{
    cout<<"Put the Value of A\t";
    input>>p.a;
    cout<<endl<<"Put the Value of B\t";
    input>>p.b;
}


int main ()
{int count;
//Algebra o(4,5);
//Algebra p(-4,-5);
// Algebra i(0,0);
 Algebra obj[5];
 for(int i=0;i<5;i++)
 {
 	cin>>obj[i];
 }
 Algebra obj1(2,3);
 count=obj1.CountEqual(obj,obj1);
 cout<<count;
}
